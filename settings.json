{
  "features": {
    "inline_completion_provider": "none"
  },
  "assistant": {
    "dock": "bottom",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-20240620"
    },
    "version": "2"
  },
  "show_inline_completions": false,
  "theme": "Solarized Light",
  "vim_mode": true,
  "ui_font_family": "Hack",
  "ui_font_size": 12,
  "buffer_font_family": "Hack",
  "buffer_line_height": "standard",
  "buffer_font_size": 11,
  "relative_line_numbers": true,
  "cursor_blink": false,
  "vertical_scroll_margin": 0,
  "unnecessary_code_fade": 0.7,
  "project_panel": {
    "dock": "right"
  },
  "terminal": {
    "dock": "bottom"
  },
  "scrollbar": {
    "show": "never"
  },
  "tab_bar": {
    "show": false
  },
  "languages": {
    "JavaScript": {
      "format_on_save": {
        "external": {
          "command": "prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"]
        }
      }
    }
  }
}
