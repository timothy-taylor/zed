[
  {
    "context": "Editor && vim_mode == normal && !VimWaiting && !menu",
    "bindings": {
      "g r": "editor::FindAllReferences",
      "g i": "editor::GoToDefinitionSplit",
      "space r n": "editor::Rename",
      "ctrl-h": ["workspace::SendKeystrokes", "ctrl-w h"],
      "ctrl-l": ["workspace::SendKeystrokes", "ctrl-w l"],
      "ctrl-k": ["workspace::SendKeystrokes", "ctrl-w k"],
      "ctrl-j": ["workspace::SendKeystrokes", "ctrl-w j"],
      "v s": "pane::SplitRight",
      "space g": "editor::ToggleGitBlame",
      "space s": "pane::DeploySearch",
      "space n": ["workspace::SendKeystrokes", "*"],
      "space b": ["workspace::SendKeystrokes", "#"],
      "space f": ["workspace::SendKeystrokes", "cmd-p"]
    }
  },
  {
    "context": "Editor && vim_mode == visual && !VimWaiting && !menu",
    "bindings": {
      "shift-k": "editor::MoveLineUp",
      "shift-j": "editor::MoveLineDown"
    }
  },
  {
    "context": "Editor && vim_mode == insert && !menu",
    "bindings": {}
  },
  {
    "context": "Dock",
    "bindings": {
      "ctrl-h": ["workspace::ActivatePaneInDirection", "Left"],
      "ctrl-l": ["workspace::ActivatePaneInDirection", "Right"],
      "ctrl-k": ["workspace::ActivatePaneInDirection", "Up"],
      "ctrl-j": ["workspace::ActivatePaneInDirection", "Down"]
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "shift-k": "project_panel::CollapseAllEntries",
      "cmd-s": "pane::DeploySearch"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "cmd-t": "workspace::NewTerminal"
    }
  }
]
